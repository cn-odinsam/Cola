{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] "
    }
  },
  "test": "test config",
  //  "ColaWebApiSettings": {
  //    "ForbiddenClientName": "default",
  //    "Decompression": "All"
  //  },
  "ColaSnowFlake": {
    "DatacenterId": 1,
    "WorkerId": 1
  },
  "ColaLogs": {
    "LogSeparator": "*",
    "LogTimeFormat": "yyyy-MM-dd HH:mm:ss",
    /*
    File,SqlServer,MySql,All   e.g.
    "SaveType":"File,MySql"
    "SaveType":"All"
    */
    "SaveType": "File",
    "DbConnectionString": "",
    "DirectoryName": "ColaLogs",
    "KeepTime": 10
  },
  "ColaOrm": {
    "TenantResolutionStrategy": "NoTenant",
    "ColaOrmConfig": [
      {
        // 多租户模式租户id传参方式      
        // NoTenant  不使用多租户   RouteValueTenant 路由形式   HttpHeaderTenant  http头形式    DomainTenant   域名形式
        // 路由形式 和 http头形式  都必须传递  tenantId：value

        // 默认值 1
        "ConfigId": "1",
        "DbType": "MySql",
        "ConnectionString": "server=47.122.0.223;Database=OdinPush;Uid=root;Pwd=173djjDJJ;AllowLoadLocalInfile=true;",
        "IsAutoCloseConnection": true,
        "EnableLogAop": true,
        "EnableErrorAop": true,
        "EnableGlobalFilter": true
      }
    ]
  },
  "ColaWebApi": [
    {
      "ClientName": "Cola",
      "BaseUri": "https://tenapi.cn",
      "TimeOut": 5000,
      "Cert": {
        "CertFilePath": "",
        "CertFilePwd": ""
      },
      /* 默认压缩方式  None,GZip,Deflate,Brotli,All */
      "Decompression": "All"
    }
  ],
  "ColaCache": {
    // 缓存类型   NoCache = 0, 不使用缓存   InMemory = 1,   使用本地缓存    Redis = 2,      使用redis缓存   Hybrid = 3      综合模式：优先使用本地缓存，再使用redis缓存
    "CacheType": 2,
    "RedisCache": {
      // 单机模式
      "Standalone": {
        "ConnectionStrings": "47.122.0.223:6379,password=173djjDJJ,ssl=False,abortConnect=True"
      },
      //      // 哨兵+集群
      //      "Sentinels": {
      //        "Sentinel": "sentinel-host:port,sentinel2-host:port?sentinel=true&password=xxxxx&db=0&ssl=False&abortConnect=False&connectTimeout=5000&allowAdmin=True",
      //        "Masters": "redis-master:port,password=xxxxx,connectTimeout=5000,allowAdmin=True",
      //        "Slaves": "redis-slave:port,password=xxxxx,connectTimeout=5000,allowAdmin=True",
      //        "ServiceName": "mymaster"
      //      },
      //      //集群模式
      //      "Cluster": {
      //        "Masters": "redis-master:port,password=xxxxx,connectTimeout=5000,allowAdmin=True",
      //        "Slaves": "redis-slave:port,password=xxxxx,connectTimeout=5000,allowAdmin=True"
      //      },
      //事件总线
      "EventBus": "47.122.0.223:6379,password=173djjDJJ,connectTimeout=5000,allowAdmin=False,ssl=False,abortConnect=True"
    },
    "MemoryCache": {
      //当缓存项的数量超过这个值时，会使用 LRU 算法移除最近最少使用的缓存项。默认值为 null，表示没有限制
      "SizeLimit": null,
      //设置扫描过期项的频率。默认值为 30 秒，可以根据需要调整。
      "ExpirationScanFrequency": 30,
      //设置缓存项数量达到最大限制后，移除缓存项的百分比。默认值为 0.1，表示移除最近最少使用的 10% 的缓存项。
      "CompactionPercentage": 0.1,
      // 本地缓存默认缓存时间
      "DefaultExpiration":30
    }
  },
  "ColaGrpcClient": {
    //如果设置为 true 且指定了 HttpMessageHandler 或 HttpClient，则在处置 GrpcChannel 时，将分别处置 HttpHandler 或 HttpClient
    "DisposeHttpClient": false,
    //可以从客户端发送的最大消息大小 单位MB
    "MaxSendMessageSize": null,
    //可以由客户端接收的最大消息大小 单位MB
    "MaxReceiveMessageSize": null,
    //如果设置为 true，则在取消调用或超过其截止时间时，客户端将引发 OperationCanceledException
    "ThrowOperationCanceledOnCancellation": false,
    //拦截器日志 默认开启
    "InterceptorLog": true
  },
  "ColaJwt": {
    "Secret": "123456",
    "IsUser": "odinsam.com",
    "Audience": "odinsam",
    "AccessExpiration": 30,
    "RefreshExpiration": 30
  }
